        <div class="container">
            <div class="row pt-5 text-success"  style="font-family: 'Playfair Display SC', serif;">

                <div class="col-md-4 justify-content-center mt-5">
                    <form action="/ranger" method="post">
                        <h1 class="text-success text-center">Ranger Registration</h1>
                        <div class="form-group mb-4">
                            <label for="rangerName" class="fs-4 mb-2">Name:</label>
                            <input type="text" class="form-control" id="rangerName" name="rangerName" required>
                        </div>
                        <div class="form-group mb-4">
                            <label for="kwsPin" class="fs-4 mb-2">KWS pin</label>
                            <input type="password" class="form-control" id="kwsPin" name="kwsPin" required>
                        </div>

                        <button class="btn btn-success d-block mx-auto" type="submit">Register</button>
                    </form>
                </div>

            </div>
        </div>



{{/partial}}

</div>

